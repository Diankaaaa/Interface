<Window x:Class="Interface.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Interface"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="551.238">
    <Grid Background="#FF252526">
        <Grid.RowDefinitions>
            <RowDefinition Height="140*"/>
            <RowDefinition Height="280*"/>
        </Grid.RowDefinitions>
        <Button Content="Кнопка" Grid.Row="0" HorizontalAlignment="Center" Width="350" Margin="40" Foreground="White" Background="#FF686868" Click="Button_Click" FontSize="16"/>
        <TextBlock x:Name="OUTPUT" Grid.Row="1" Margin="20" Foreground="White" Background="#FF686868" FontSize="16"/>
    </Grid>
</Window>
